{
	"schemaVersion": 1,
	"id": "animcreator",
	"version": "${version}",
	"name": "AnimCreator",
	"description": "Mod to make animations with blocks",
	"authors": [
		"Astralio"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/animcreator/icon.png",
